module main;

import io;

struct Example {
    field: str,
    xyz: bool
};

struct Point {
    x: i32,
    y: i32
};

fn mid(p1: Point, p2: Point): Point {
    ret := Point {
        x = (p1.x + p2.x) / 2,
        y = (p1.y + p2.y) / 2
    };
    ret
};

fn main() {
    mut var := Example {
        field = "test",
        xyz = false
    };
    var.field = "changed";
    println(var.field);
};
