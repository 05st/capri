module io;

extern printf(char*, char*): unit;
extern scanf(char*, char*): unit;
extern malloc(i32): i32;

pub fn print(string: char*): unit
    printf("%s", string);

pub fn println(string: char*): unit {
    printf("%s\n", string);
};

pub fn input(): char* {
    buffer := (char*)malloc(256);
    scanf("%s", buffer);
    buffer
};
